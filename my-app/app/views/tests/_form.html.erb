<%= simple_form_for(@test) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.association :subject %>
      <%= f.input :title %>
      <%= f.input :questions %>
    </div>

    <div class="form-actions">
      <%= f.button :magic_submit, cancel: tests_path %>
    </div>
<% end %>